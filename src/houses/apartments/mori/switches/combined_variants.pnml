// Basic Colors
switch (FEAT_HOUSES, SELF, switch_mori_a_item_basic_colors, random_bits % 16 ) {
	0: switch_mori_a_height1_mauve_snow;
	1..2: switch_mori_a_height1_brown2_snow;
	3..4: switch_mori_a_height1_brown1_snow;
	5..6: switch_mori_a_height1_grey_snow;
	7: switch_mori_a_height1_white_snow;
	8: switch_mori_a_height2_mauve_snow;
	9..10: switch_mori_a_height2_brown2_snow;
	11..12: switch_mori_a_height2_brown1_snow;
	13..14: switch_mori_a_height2_grey_snow;
	15: switch_mori_a_height2_white_snow;
}

// Modern Colors
switch (FEAT_HOUSES, SELF, switch_mori_a_item_modern_colors, random_bits % 32 ) {
	0..1: 	switch_mori_a_height1_mauve_snow;
	2..4: 	switch_mori_a_height1_brown2_snow;
	5..7: 	switch_mori_a_height1_brown1_snow;
	8..10: 	switch_mori_a_height1_grey_snow;
	11: 	switch_mori_a_height1_white_snow;
	12: 	switch_mori_a_height1_dark_green_snow;
	13: 	switch_mori_a_height1_peach_snow;
	14: 	switch_mori_a_height1_light_blue_snow;
	15: 	switch_mori_a_height1_dark_blue_snow;
	16..17: switch_mori_a_height2_mauve_snow;
	18..20: switch_mori_a_height2_brown2_snow;
	21..23: switch_mori_a_height2_brown1_snow;
	24..26: switch_mori_a_height2_grey_snow;
	27: 	switch_mori_a_height2_white_snow;
	28: 	switch_mori_a_height2_dark_green_snow;
	29: 	switch_mori_a_height2_peach_snow;
	30: 	switch_mori_a_height2_light_blue_snow;
	31: 	switch_mori_a_height2_dark_blue_snow;
}

// Which Period?
switch (FEAT_HOUSES, SELF, switch_mori_a_item_snow, current_year - age) {
	0..1959: switch_mori_a_item_basic_colors;
	switch_mori_a_item_modern_colors;
}
