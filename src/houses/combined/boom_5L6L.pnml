// Construction Check
switch (FEAT_HOUSES, SELF, switch_boom_5L6L_con_check, 	
	FuncServicePASS() &&				// Only build within the catchment area of a station which received passengers this month or last month
	(PopOver5L6LThreshold() && !PopOver7L8LThreshold() ) ||
	(PopOver7L8LThreshold() && !WideRoads() )
	) {return;}	

item(FEAT_HOUSES, item_boom_5L6L){
	property {
		substitute: 				27;
		name: 						string(NAME_APARTMENTS_YANO);
		population: 				100;
		accepted_cargos: 			[[PASS, 4],[MAIL, 2]];
		local_authority_impact: 	80;
		removal_cost_multiplier: 	80;
		probability: 				2;
		years_available: 			[1960,1979];
		minimum_lifetime: 			5;
		availability_mask: 			[ALL_TOWNZONES & ~bitmask(TOWNZONE_EDGE), ALL_CLIMATES | bitmask(ABOVE_SNOWLINE)];
		building_class:				4;
				
		}
	graphics {
		name:						switch_boom_5L6L_name;
		default: 					switch_boom_5L6L_sprites;
		construction_check: 		switch_boom_5L6L_con_check;
		cargo_production: 			func_produce(10,4);
		colour: 					switch_boom_5L6L_colors;
	}
}
