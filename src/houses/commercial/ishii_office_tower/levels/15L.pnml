
// _white_
    // Norm Sprites
    spritelayout sprlay_ishii_office_tower_a_15L_white_norm {
        ground {
            sprite: spr_ishii_office_tower_a_ground (3);
        }
        building {
            sprite: spr_ishii_office_tower_a_15L_norm (3);
            recolour_mode: RECOLOUR_REMAP; 
            palette: recolour_remap + 0;
            xoffset: 0;
            yoffset: 0;
        }
    }

    // Snow Sprites
    spritelayout sprlay_ishii_office_tower_a_15L_white_snow {
        ground {
            sprite: spr_ishii_office_tower_a_ground_snow (3);
        }
        building {
            sprite: spr_ishii_office_tower_a_15L_snow (3);
            recolour_mode: RECOLOUR_REMAP; 
            palette: recolour_remap + 0;
            xoffset: 0;
            yoffset: 0;
        }
    }

    switch(FEAT_HOUSES, SELF, switch_ishii_office_tower_a_15L_white_snow,terrain_type) {
	    TILETYPE_SNOW: sprlay_ishii_office_tower_a_15L_white_snow;
	    sprlay_ishii_office_tower_a_15L_white_norm;
    }

