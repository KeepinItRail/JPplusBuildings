
// _black_
    // Norm Sprites
    spritelayout sprlay_old_office_building_a_7L_black_norm {
        ground {
            sprite: spr_old_office_building_a_ground (construction_state);
        }
        building {
            sprite: spr_old_office_building_a_7L_norm (construction_state);
            recolour_mode: RECOLOUR_REMAP; 
            palette: recolour_remap + 11;
            xoffset: 0;
            yoffset: 0;
        }
    }

    // Snow Sprites
    spritelayout sprlay_old_office_building_a_7L_black_snow {
        ground {
            sprite: spr_old_office_building_a_ground_snow (construction_state);
        }
        building {
            sprite: spr_old_office_building_a_7L_snow (construction_state);
            recolour_mode: RECOLOUR_REMAP; 
            palette: recolour_remap + 11;
            xoffset: 0;
            yoffset: 0;
        }
    }

    switch(FEAT_HOUSES, SELF, switch_old_office_building_a_7L_black_snow,terrain_type) {
	    TILETYPE_SNOW: sprlay_old_office_building_a_7L_black_snow;
	    sprlay_old_office_building_a_7L_black_norm;
    }

