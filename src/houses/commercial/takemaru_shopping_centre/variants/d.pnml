
// _dark_green_
    // Norm Sprites
    spritelayout sprlay_takemaru_shopping_centre_d_xL_dark_green_norm {
        ground {
            sprite: spr_takemaru_shopping_centre_d_ground (construction_state);
        }
        building {
            sprite: spr_takemaru_shopping_centre_d_xL_norm (construction_state);
            recolour_mode: RECOLOUR_REMAP; 
            palette: recolour_remap + 5;
            xoffset: 0;
            yoffset: 0;
        }
    }

    // Snow Sprites
    spritelayout sprlay_takemaru_shopping_centre_d_xL_dark_green_snow {
        ground {
            sprite: spr_takemaru_shopping_centre_d_ground_snow (construction_state);
        }
        building {
            sprite: spr_takemaru_shopping_centre_d_xL_snow (construction_state);
            recolour_mode: RECOLOUR_REMAP; 
            palette: recolour_remap + 5;
            xoffset: 0;
            yoffset: 0;
        }
    }

    switch(FEAT_HOUSES, SELF, switch_takemaru_shopping_centre_d_xL_dark_green_snow,terrain_type) {
	    TILETYPE_SNOW: sprlay_takemaru_shopping_centre_d_xL_dark_green_snow;
	    sprlay_takemaru_shopping_centre_d_xL_dark_green_norm;
    }

