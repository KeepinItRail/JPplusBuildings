1: switch_ishii_office_tower_11L_grey_snow;
2..3: switch_ishii_office_tower_11L_brown1_snow;
4..5: switch_ishii_office_tower_11L_brown2_snow;
6: switch_ishii_office_tower_11L_mauve_snow;
7: switch_ishii_office_tower_11L_dark_blue_snow;
8..9: switch_ishii_office_tower_11L_black_snow;
10: switch_ishii_office_tower_11L_gold_snow;
11: switch_ishii_office_tower_11L_red_brown_snow;
}

