	0: 		switch_murakami_a_3L_mauve_snow;
	1..2: 	switch_murakami_a_3L_brown2_snow;
	3..4: 	switch_murakami_a_3L_brown1_snow;
	5..6: 	switch_murakami_a_3L_grey_snow;
	7: 		switch_murakami_a_3L_white_snow;
	8: 		switch_murakami_a_4L_mauve_snow;
	9..10: 	switch_murakami_a_4L_brown2_snow;
	11..12: switch_murakami_a_4L_brown1_snow;
	13..14: switch_murakami_a_4L_grey_snow;
	15: 	switch_murakami_a_4L_white_snow;
}

